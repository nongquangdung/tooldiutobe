class AudioPreviewThread(QThread):
    """Thread ƒë·ªÉ generate preview √¢m thanh th·∫≠t"""
    preview_completed = Signal(str, str, bool)  # emotion_name, audio_path, success
    preview_progress = Signal(str, int)  # emotion_name, progress_percent
    preview_error = Signal(str, str)  # emotion_name, error_message
    
    def __init__(self, emotion_name: str, parameters: dict):
        super().__init__()
        self.emotion_name = emotion_name
        self.parameters = parameters
        # D√πng prompt ng·∫Øn g·ªçn; n·∫øu c·∫ßn c√≥ b·∫£ng EMOTION_PROMPTS c√≥ th·ªÉ n√¢ng c·∫•p sau
        self.preview_text = f"This is a preview of the {emotion_name} emotion with custom parameters."
    
    def run(self):
        """Generate emotion preview audio using RealChatterboxProvider (if available)"""
        try:
            # 10% ‚Äì b·∫Øt ƒë·∫ßu
            self.preview_progress.emit(self.emotion_name, 10)

            if PREVIEW_AVAILABLE:
                try:
                    provider = RealChatterboxProvider()
                    self.preview_progress.emit(self.emotion_name, 30)

                    # B·∫£o ƒë·∫£m t√™n file duy nh·∫•t tr√°nh xung ƒë·ªôt/permission denied
                    preview_dir = "test_audio_output"
                    os.makedirs(preview_dir, exist_ok=True)
                    import time as _preview_time
                    ts = int(_preview_time.time())
                    audio_path = os.path.join(preview_dir, f"emotion_preview_{self.emotion_name}_{ts}.wav")

                    # 60% ‚Äì ƒëang generate
                    self.preview_progress.emit(self.emotion_name, 60)

                    result = provider.generate_voice(
                        text=self.preview_text,
                        save_path=audio_path,
                        emotion_exaggeration=self.parameters.get("exaggeration", 1.0),
                        cfg_weight=self.parameters.get("cfg_weight", 0.5),
                        speed=self.parameters.get("speed", 1.0),
                    )

                    success = result.get("success", False)
                    # 100% ‚Äì ho√†n t·∫•t
                    self.preview_progress.emit(self.emotion_name, 100)

                    if success and os.path.exists(audio_path):
                        self.preview_completed.emit(self.emotion_name, audio_path, True)
                    else:
                        self.preview_error.emit(self.emotion_name, "TTS generation failed")

                except Exception as gen_err:
                    self.preview_error.emit(self.emotion_name, f"TTS error: {gen_err}")
            else:
                # Simulation mode n·∫øu thi·∫øu provider
                import time as _sim_time
                _sim_time.sleep(2)
                self.preview_progress.emit(self.emotion_name, 100)
                self.preview_completed.emit(self.emotion_name, "simulated_preview.wav", False)

        except Exception as e:
            self.preview_error.emit(self.emotion_name, str(e))
    
class InnerVoicePreviewThread(QThread):
    """SIMPLIFIED: Generate TTS then apply FFmpeg effects"""
    preview_completed = Signal(str, str, bool)  # inner_voice_type, audio_path, success
    preview_error = Signal(str, str)  # inner_voice_type, error_message
    preview_progress = Signal(str, int)  # inner_voice_type, progress_percent
    
    def __init__(self, inner_voice_type: str, parameters: dict):
        super().__init__()
        self.inner_voice_type = inner_voice_type
        self.parameters = parameters
        
        # Sample texts for each type
        self.texts = {
            'light': 'This is a preview of the light emotion with custom effects applied.',
            'deep': 'The deep voice resonates with powerful undertones and rich harmonics.',
            'dreamy': 'Cotton candy clouds carry wishes to sleeping mountains far away.'
        }
    
    def run(self):
        """SIMPLE: 2-step process - TTS then FFmpeg"""
        try:
            import time
            import subprocess
            import os
            
            print(f"üéµ [SIMPLE] Starting preview: {self.inner_voice_type}")
            
            # Step 1: Generate base TTS
            text = self.texts.get(self.inner_voice_type, self.texts['light'])
            timestamp = int(time.time())
            base_file = f"test_audio_output/base_{self.inner_voice_type}_{timestamp}.wav"
            
            os.makedirs("test_audio_output", exist_ok=True)
            
            # Use RealChatterboxProvider directly (same as AudioPreviewThread)
            try:
                provider = RealChatterboxProvider()
                
                result = provider.generate_voice(
                    text=text,
                    save_path=base_file,
                    emotion_exaggeration=1.0,
                    cfg_weight=0.5,
                    speed=1.0
                )
                success = result.get('success', False)
                
                if not success or not os.path.exists(base_file):
                    self.preview_error.emit(self.inner_voice_type, "Failed to generate base TTS")
                    return
                    
                print(f"‚úÖ [SIMPLE] Base TTS: {base_file}")
                
            except Exception as e:
                self.preview_error.emit(self.inner_voice_type, f"TTS error: {e}")
                return
            
            # Step 2: Apply Inner Voice effects using simple processor
            output_file = f"test_inner_voice_output/preview_{self.inner_voice_type}_{timestamp}.wav"
            os.makedirs("test_inner_voice_output", exist_ok=True)
            
            try:
                from core.inner_voice_simple import apply_inner_voice_effects
                
                success = apply_inner_voice_effects(
                    input_file=base_file,
                    output_file=output_file,
                    voice_type=self.inner_voice_type,
                    custom_params=self.parameters
                )
                
                if success and os.path.exists(output_file):
                    print(f"‚úÖ [SIMPLE] Effects applied: {output_file}")
                    
                    # Cleanup base file
                    try:
                        os.remove(base_file)
                    except:
                        pass
                    
                    self.preview_completed.emit(self.inner_voice_type, output_file, True)
                else:
                    self.preview_error.emit(self.inner_voice_type, "Failed to apply inner voice effects")
                    
            except Exception as e:
                print(f"‚ùå [SIMPLE] Effects error: {e}")
                self.preview_error.emit(self.inner_voice_type, f"Effects error: {e}")
                
        except Exception as e:
            print(f"‚ùå [SIMPLE] Preview error: {e}")
            self.preview_error.emit(self.inner_voice_type, f"Preview error: {e}")

