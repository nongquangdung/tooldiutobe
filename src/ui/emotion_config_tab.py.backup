    def preview_inner_voice(self, inner_voice_type: str):
        """SIMPLIFIED: Preview inner voice effect v·ªõi parameters hi·ªán t·∫°i"""
        try:
            print(f"üü° [SIMPLE] preview_inner_voice called with type={inner_voice_type}")
            
            # Check if already running
            if inner_voice_type in self.inner_voice_preview_threads and self.inner_voice_preview_threads[inner_voice_type]:
                if self.inner_voice_preview_threads[inner_voice_type].isRunning():
                    self.update_status(f"‚è≥ Inner voice {inner_voice_type} ƒëang ƒë∆∞·ª£c t·∫°o...")
                    return
            
            # Get current parameters t·ª´ UI
            if inner_voice_type not in self.inner_voice_type_widgets:
                self.update_status(f"‚ùå Kh√¥ng t√¨m th·∫•y widget cho {inner_voice_type}")
                return
                
            widgets = self.inner_voice_type_widgets[inner_voice_type]
            
            parameters = {
