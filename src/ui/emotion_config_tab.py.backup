    def filter_emotions(self):
        """L·ªçc emotions theo category v√† custom"""
        category_filter = self.category_filter.currentText()
        show_custom_only = self.show_custom_only.isChecked()
        
        for row in range(self.emotions_table.rowCount()):
            name_item = self.emotions_table.item(row, 0)
            if name_item:
                emotion_name = name_item.text()
                emotion = self.unified_emotion_system.get_all_emotions().get(emotion_name)
                
                if emotion:
                    # Category filter - s·ª≠a l·∫°i ƒë·ªÉ ƒë√∫ng c·∫•u tr√∫c
                    emotion_category = emotion.get('category', 'neutral')
                    category_match = (category_filter == "T·∫•t c·∫£" or emotion_category == category_filter)
                    
                    # Custom filter - ki·ªÉm tra xem c√≥ ph·∫£i custom emotion kh√¥ng
                    is_custom = self.unified_emotion_system.is_custom_emotion(emotion_name)
                    custom_match = (not show_custom_only or is_custom)
                    
                    # Show/hide row
                    show_row = category_match and custom_match
                    self.emotions_table.setRowHidden(row, not show_row)
    
    def sort_emotions_by_category(self):
        """Sort emotions theo category trong b·∫£ng"""
        try:
            # L·∫•y t·∫•t c·∫£ emotions v√† sort theo category
            all_emotions = self.unified_emotion_system.get_all_emotions()
            
            # Define category order
            category_order = ["neutral", "positive", "negative", "dramatic", "authoritative", 
                            "special", "surprise", "desperate", "nervous", "mysterious", 
                            "urgent", "sarcastic", "confused", "innocent"]
            
            # Sort emotions theo category order
            sorted_emotions = []
            for category in category_order:
                # Get emotions for this category
                category_emotions = []
                for emotion_name, emotion in all_emotions.items():
                    if emotion.get('category', 'neutral') == category:
                        category_emotions.append((emotion_name, emotion))
                
                # Sort alphabetically within category
                category_emotions.sort(key=lambda x: x[0])
                sorted_emotions.extend(category_emotions)
            
            # Add any remaining emotions not in predefined categories
            remaining_emotions = []
            for emotion_name, emotion in all_emotions.items():
                emotion_category = emotion.get('category', 'neutral')
                if emotion_category not in category_order:
                    remaining_emotions.append((emotion_name, emotion))
            
            remaining_emotions.sort(key=lambda x: x[0])
            sorted_emotions.extend(remaining_emotions)
            
            # Clear v√† reload table v·ªõi sorted emotions
            self.emotions_table.setRowCount(0)
            self.emotions_table.setRowCount(len(sorted_emotions))
            
            row = 0
            for emotion_name, emotion in sorted_emotions:
                # Column 0: Emotion Name
                name_item = QTableWidgetItem(emotion_name)
