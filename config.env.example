# ===========================================
# API KEYS - Copy to config.env and fill in your keys
# ===========================================

# AI Content Generation
OPENAI_API_KEY=your_openai_api_key_here
CLAUDE_API_KEY=your_claude_api_key_here
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# Image Generation
# DALL_E_API_KEY sử dụng chung OPENAI_API_KEY
MIDJOURNEY_API_KEY=your_midjourney_api_key_here
STABILITY_AI_KEY=your_stability_ai_key_here

# Text-to-Speech
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here
GOOGLE_TTS_API_KEY=your_google_tts_api_key_here
AZURE_SPEECH_KEY=your_azure_speech_key_here
AZURE_SPEECH_REGION=eastus

# ===========================================
# PROVIDER PREFERENCES
# ===========================================
CONTENT_PROVIDER=OpenAI GPT-4
IMAGE_PROVIDER=DALL-E (OpenAI)
TTS_PROVIDER=Google TTS (Free)

# ===========================================
# SETTINGS
# ===========================================
DEFAULT_VOICE=alloy
DEFAULT_LANGUAGE=vi
VIDEO_RESOLUTION=1920x1080
VIDEO_FPS=25 